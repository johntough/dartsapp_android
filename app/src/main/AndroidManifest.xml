<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="murrayfield.sportsbar.dartsapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayerActivity"
            android:label="@string/title_activity_player" >
        </activity>
        <activity
            android:name=".FixtureActivity"
            android:label="@string/title_activity_fixture" >
        </activity>
        <activity
            android:name=".WeekActivity"
            android:label="@string/title_activity_week" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_results" >
        </activity>
        <activity
            android:name=".TableActivity"
            android:label="@string/title_activity_table" >
        </activity>
        <activity
            android:name=".Player180Activity"
            android:label="@string/title_activity_player180" >
        </activity>
        <activity
            android:name=".HighFinishActivity"
            android:label="@string/title_activity_high_finish" >
        </activity>
        <activity
            android:name=".BestLegActivity"
            android:label="@string/title_activity_best_leg" >
        </activity>
    </application>

</manifest>
